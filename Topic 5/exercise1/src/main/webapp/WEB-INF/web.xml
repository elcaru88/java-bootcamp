<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app
version="3.0"
    xmlns="http://java.sun.com/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 
    http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>InsertServlet</servlet-name>
  	<display-name>InsertServlet</display-name>
  	<description></description>
  	<servlet-class>controler.InsertServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>CourseListServlet</servlet-name>
  	<display-name>CourseListServlet</display-name>
  	<description></description>
  	<servlet-class>controler.CourseListServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>StudentNoteServlet</servlet-name>
  	<display-name>StudentNoteServlet</display-name>
  	<description></description>
  	<servlet-class>controler.StudentNoteServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>InsertServlet</servlet-name>
  	<url-pattern>/InsertServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>CourseListServlet</servlet-name>
  	<url-pattern>/CourseListServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>StudentNoteServlet</servlet-name>
  	<url-pattern>/StudentNoteServlet</url-pattern>
  </servlet-mapping>
  
   <welcome-file-list>
  	<welcome-file>Menu.html</welcome-file>
  </welcome-file-list>
  
</web-app>
